package com.basamadco.opxi.callmanager.call.route;

import com.basamadco.opxi.callmanager.OpxiException;

/**
 * Use this exception when you want the whole routing process to stop and notify and error
 * 
 * @author Jrad
 *         Date: Nov 19, 2007
 *         Time: 10:28:40 AM
 */
public class CallRouteException extends OpxiException {


    public CallRouteException( String msg ) {
        super( msg );
    }

    public CallRouteException( Throwable t ) {
        super( t.getMessage(), t );
    }
}
